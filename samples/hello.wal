use($wasi "wasi_unstable")

data $ret (i32)
data $ciovec (i32 $ptr i32 $len)
data $str "Hello, W0rld!\n"

func $main export()
  {($ciovec:ptr) *= ($str)}
  {($ciovec:len) *= (size.of $str)}
  $wasi:fd_write
    1 ;; fd: stdout
    ($ciovec) ;; const ciovec*
    1 ;; len of ciovec
    ($ret) ;; written*
  drop
